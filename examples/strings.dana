node NameSource {
    out name: String = "Dana"
}

node Greeter {
    prefix: String = "Welcome, "
    
    in name: String
    out greeting: String
    
    process: (name) => {
        emit greeting(prefix + name + "!")
    }
}

NameSource.name -> Greeter.name
Greeter.greeting -> System.IO.stdout
